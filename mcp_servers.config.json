{
  "version": "1.0",
  "servers": {
    "google-sheets": {
      "repository": "https://github.com/xing5/mcp-google-sheets.git",
      "build_command": "uv sync",
      "command": "uv",
      "args": [
        "run",
        "--directory",
        "/tmp/mcp-servers/google-sheets",
        "python",
        "src/mcp_google_sheets/server.py"
      ]
    }
  }
}
